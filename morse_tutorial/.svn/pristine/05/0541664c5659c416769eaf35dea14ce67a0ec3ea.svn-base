<solver driver="-m" extension="">
	<bin id="solver" name="test_nd_hmat"/>
	<bin id="verif" name="echo"/>
	<test id="hmat_oss_examples"/>
	<options default="--stats"/>
	<methods>
		<method id="default" options="" name="Binary Nested Dissection"/>
		<method id="bind-comp" options="--diff 2" name="Binary Nested Dissection"/>
		<method id="ternd" options="--ternd" name="Ternary Nested Dissection"/>
		<method id="dense" options="--dense" name="Dense original solver"/>
	</methods>
	<patterns>
		<pattern id="memory" text="^[ ]*Memory used after factorization" />
		<pattern id="facto" text="timer LU" />
		<pattern id="solve" text="timer Solution" />
		<pattern id="nnz" text="Number of nonzeros \(local block structure\)" />
	</patterns>
</solver>